<resources>
    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/azul</item>
        <item name="colorPrimaryDark">@color/azul</item>
        <item name="colorAccent">@color/azul</item>
    </style>

    <style name="AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">@color/azul</item>
        <item name="colorPrimary">@color/azul</item>
        <item name="colorAccent">@color/rojo</item>
    </style>

    <style name="SplashTheme1" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/azul</item>
        <item name="colorPrimaryDark">@color/azul</item>
        <item name="colorAccent">@color/azul</item>
        <item name="android:windowBackground">@drawable/splashscreen1</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.NoActionBar2" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/backgroud</item>
        <item name="colorPrimaryDark">@color/backgroud</item>
        <item name="colorAccent">@color/rojo</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>